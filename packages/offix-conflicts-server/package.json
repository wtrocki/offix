{
  "name": "offix-conflicts-server",
  "version": "0.5.0",
  "description": "Offix GraphQL server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "test": "ava '*.test.ts' '**/*.test.ts'",
    "compile:clean": "tsc --build tsconfig.json --clean"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aerogear/offix.git"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*"
    ],
    "sources": [
      "src/**/*"
    ]
  },
  "devDependencies": {
    "ava": "2.1.0",
    "graphql": "14.3.1",
    "ts-node": "8.1.0",
    "typescript": "3.4.4"
  },
  "peerDependencies": {
    "graphql": "14.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
